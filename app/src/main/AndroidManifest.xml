<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsmastrodomenico.contactlistapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/agenda"
        android:label="@string/app_name"
        android:roundIcon="@drawable/agenda"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainMenu"></activity>
        <activity android:name=".CreateContact"></activity>
        <activity android:name=".ListContact"></activity>
        <activity android:name=".ContactDetail"></activity>
        <activity android:name=".LoginAccess">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>